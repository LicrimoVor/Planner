{
  "name": "Planner",
  "version": "1.0.0",
  "description": "",
  "main": "dist/init.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean & tsc & npx fix-esm-import-path dist",
    "build_prod": "tsc & npx fix-esm-import-path dist"
